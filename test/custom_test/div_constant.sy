int main() {
  int a[20];
  int n = getarray(a);
  int i = 0;
  while (i < n) {
    putint(a[i] / 1); putch(10);
    putint(a[i] / 3); putch(10);
    putint(a[i] / 6); putch(10);
    putint(a[i] / 7); putch(10);
    putint(a[i] / 14); putch(10);
    putint(a[i] / 31); putch(10);
    putint(a[i] / 641); putch(10);
    putint(a[i] / 0x7FFFFFFF); putch(10);
    putint(a[i] / -2147483648); putch(10);
    // bitset1
    putint(a[i] / 1000000007); putch(10);
    // power of two for positive integers
    if (a[i] > 0) {
      putint(a[i] / 2); putch(10);
      putint(a[i] / 4); putch(10);
      putint(a[i] / 16); putch(10);
      putint(a[i] / 32); putch(10);
      putint(a[i] / 1024); putch(10);
      putint(a[i] / 65536); putch(10);
      putint(a[i] / 1048576); putch(10);
    }
    putch(10);
    i = i + 1;
  }
  return 0;
}